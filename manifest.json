{
    "manifest_version": 2,
    "name": "DIM cumunity aditions",
    "version": "1.1.2",
    "browser_action":{
        "default_popup": "button.html"
    },
    "content_scripts": [
        {
            "matches": ["https://app.destinyitemmanager.com/*"],
            "js": ["content.js", "weaponFilter.js", "jquery-3.6.0.min.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": [
        "tabs",
        "https://app.destinyitemmanager.com/*"
    ]
}